{
  "name": "agora",
  "version": "1.0.0",
  "description": "Your go-to e-commerce web app for selling whatever you want",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "git checkout -b deploy && webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push --force heroku deploy:master && git checkout master && git branch -D deploy",
    "start": "node server",
    "start-dev": "npm run build-client-watch & npm run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/agora_test' mocha ./**/*.spec.js --compilers js:babel-register",
    "test-models": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/agora_test' mocha -w ./server/db/models/*.spec.js --compilers js:babel-register",
    "test-api": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/agora_test' mocha -w ./server/api/*.spec.js --compilers js:babel-register",
    "lint-fix": "eslint --fix -c .eslintrc.js .",
    "seed": "node ./server/db/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.16.1",
    "bootstrap-sass": "^3.3.7",
    "chalk": "^2.1.0",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "faker": "^4.1.0",
    "history": "^4.6.3",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.4",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.3.1",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.0",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.0",
    "source-map": "^0.5.7",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "webpack": "^2.7.0"
  }
}
